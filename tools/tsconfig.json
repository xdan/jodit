{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 20",

	"_version": "20.16.0",

	"compilerOptions": {
		"lib": ["es2023"],
		"module": "node16",
		"target": "es2022",

		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
    "resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node16"
	}
}
